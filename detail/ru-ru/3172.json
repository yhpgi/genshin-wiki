        "desc": [
          {
            "type": "RichText",
            "text": "<color=#ffd780>Обычная атака</color>\nНаносит <color=#99ffff>Крио урон</color> , 2 ед., наносит <color=#ffffff>Проникающий у</color> <color=#ffffff>рон, 2 ед.</color> всем неактивным персонажам."
          }
        ],
        "attributes": []
      },
