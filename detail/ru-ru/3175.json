        "desc": [
          {
            "type": "RichText",
            "text": "<color=#ffd780>Взрыв стихии</color>\nНаносит <color=#99ffff>Крио урон</color> , 1 ед. и создаёт <color=#ffffff>Льдинка</color> ."
          }
