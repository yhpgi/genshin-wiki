        "desc": [
          {
            "type": "RichText",
            "text": "<color=#ffd780>Обычная ат</color> <color=#ffd780>ака</color>\nНаносит <color=#ffffff>Физический урон</color> , 2 ед."
          }
        ],
        "attributes": []
