        "desc": [
          {
            "type": "RichText",
            "text": "<color=#ffd780>Beschwörung</color>\n<color=#ffffff>Endphase:</color> Verursacht 2 Pkt. <color=#80c0ff>Hydro-SCH</color> .\n<color=#ffffff>Einsatzmöglichkeit: 2</color> (unbegrenzt stapelbar)"
          }
