          {
            "type": "RichText",
            "text": "<color=#ffd780>Elementarfähigkeit</color>\n<color=#ffffff>Vorbereiten der Fähigkeiten</color> : <color=#ffffff>Faust der Brüderlichkeit - Aufgeladen</color>"
          }
        ],
