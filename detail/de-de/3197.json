        "desc": [
          {
            "type": "RichText",
            "text": "<color=#ffd780>Beschwörung</color>\n<color=#ffffff>Endphase</color> : Verursacht 1 Pkt. <color=#99ffff>Kryo-SCH</color> .\n<color=#ffffff>Einsat</color> <color=#ffffff>zmöglichkeit: 2</color>"
