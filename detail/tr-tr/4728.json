        "desc": [
          {
            "type": "RichText",
            "text": "<color=#ffd780>Normal Saldırı</color>\n<color=#ffffff73>2</color> <color=#ffffff>Fiziksel Hasar</color> <color=#ffffff73>verir.</color>"
          }
